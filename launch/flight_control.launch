<launch>
  <arg name="trajectory_file" default="$(find trajectory_gen)/trajectories/csv_trajectories.yaml"/>

  <node name="trajectory_generator"
        pkg="trajectory_gen"
        type="flight_control_node.py"
        clear_params="true"
        output="screen">

    <rosparam file="$(arg trajectory_file)" />
    <rosparam file="$(find trajectory_gen)/config/start_mode.yaml" />
  </node>
</launch>